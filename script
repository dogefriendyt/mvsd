game:GetService("StarterGui"):SetCore("SendNotification",{
                Title = "Welcome!";
                Text = "Enjoy using Doge Hub";
                Duration = 1.5;})


local Library = loadstring(game:HttpGet("https://pastebin.com/raw/GdN5iRbt"))() 
local Window = Library.CreateLib("Doge Hub", "DarkTheme") 


local Tab1 = Window:NewTab("Scripts")
local Tab1Section = Tab1:NewSection("Good Stuff")


Tab1Section:NewButton("Esp", "Loads the hub.", function()
game:GetService("StarterGui"):SetCore("SendNotification",{
                Title = "Notification";
                Text = "Loaded!";
                Duration = 0.5;})
loadstring(game:HttpGet("https://raw.githubusercontent.com/dogefriendyt/dump/main/esp", true))()

end)

Tab1Section:NewButton("75 Speed", "Copies the list.", function()
game:GetService("StarterGui"):SetCore("SendNotification",{
                Title = "Notification";
                Text = "Copied to clipboard.";
                Duration = 0.5;})
game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = 65

end)
